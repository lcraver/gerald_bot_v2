<h3>Point Leadboard</h3>

<div class="bar-graph dashboard_points">
  <div ng-repeat="point in points track by $index" style="width:calc(100%/{[{points.length}]}); left: calc((100%/{[{points.length}]})*{[{$index}]});">
    <div class="graph-bar" style="
    height: calc(100%*{[{point.points/points[0].points}]})">
    </div>
  </div>
</div>
<div class="bar-graph-names dashboard_points_names">
  <div ng-repeat="point in points track by $index" style="width:calc(100%/{[{points.length}]} - 16px); left: calc((100%/{[{points.length}]})*{[{$index}]});">
    {[{point.user}]}
  </div>
</div>

<h3>Active Users</h3>

<ul>
  <li ng-repeat="user in users | orderBy: username">
    {[{user.username}]}
  </li>
</ul>
